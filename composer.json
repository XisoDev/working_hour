{
    "name": "xisodev/working_hours",
    "description": "Schedule is a package that helps tracking schedules for your models. If you have workers in a company, you can set schedules for them and see their availability though the time.",
    "keywords": [
        "schedule",
        "scheduler",
        "timetable",
        "spatie",
        "opening-hours",
        "schedule",
        "opening",
        "hours"
    ],
    "homepage": "https://github.com/XisoDev/working_hour",
    "license": "MIT",
    "authors": [
        {
            "name": "XisoDev",
            "email": "ceo@amuz.co.kr",
            "homepage": "https://amuz.co.kr",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "9.*",
        "spatie/opening-hours": "^2.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "XisoDev\\WorkingHours\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XisoDev\\WorkingHours\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "XisoDev\\WorkingHours\\ScheduleServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
